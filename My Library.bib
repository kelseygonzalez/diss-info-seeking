@article{ramirezInformationSeekingMedia2013,
  title = {Information {{Seeking From Media}} and {{Family}}/{{Friends Increases}} the {{Likelihood}} of {{Engaging}} in {{Healthy Lifestyle Behaviors}}},
  author = {RAMÍREZ, A. SUSANA and FRERES, DEREK and MARTINEZ, LOURDES S. and LEWIS, NEHAMA and BOURGOIN, ANGEL and KELLY, BRIDGET J. and LEE, CHUL-JOO},
  date = {2013},
  journaltitle = {Journal of health communication},
