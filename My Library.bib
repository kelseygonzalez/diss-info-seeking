@article{johnsonComprehensiveModelCancerRelated1993,
  ids = {johnson_meischke93a,johnson_meischke93b,johnson_meischke93c},
  title = {A {{Comprehensive Model}} of {{Cancer-Related Information Seeking Applied}} to {{Magazines}}},
  author = {Johnson, J. David and Meischke, Hendrika},
  date = {1993},
  journaltitle = {Human Communication Research},
  volume = {19},
  number = {3},
