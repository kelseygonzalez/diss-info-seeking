@article{mottaRepublicansNotDemocrats2021,
  title = {Republicans, {{Not Democrats}}, {{Are More Likely}} to {{Endorse Anti-Vaccine Misinformation}}},
  author = {Motta, Matthew},
  date = {2021-09-01},
  journaltitle = {American Politics Research},
  shortjournal = {American Politics Research},
  volume = {49},
  number = {5},
  pages = {428--438},
  publisher = {{SAGE Publications Inc}},
  issn = {1532-673X},
  doi = {10.1177/1532673X211022639},
  url = {https://doi.org/10.1177/1532673X211022639},
  urldate = {2021-09-14},
  abstract = {Vaccine safety skeptics are often thought to be more likely to self-identify as Democrats (vs. Independents or Republicans). Recent studies, however, suggest that childhood vaccine misinformation is either more common among Republicans, or is uninfluenced by partisan identification (PID). Uncertainty about the partisan underpinnings of vaccine misinformation acceptance is important, as it could complicate efforts to pursue pro-vaccine health policies. I theorize that Republicans should be more likely to endorse anti-vaccine misinformation, as they tend to express more-negative views toward scientific experts. Across six demographically and nationally representative surveys, I find that—while few Americans think that “anti-vaxxers” are more likely to be Republicans than Democrats—Republican PID is significantly associated with the belief that childhood vaccines can cause autism. Consistent with theoretical expectations, effect is strongly mediated by anti-expert attitudes—an effect which supplemental panel analyses suggest is unlikely to be reverse causal.},
  langid = {english},
  keywords = {misinformation,partisanship,political psychology,public opinion,vaccine skepticism}
}



@article{mottaIdentifyingPrevalenceCorrelates2021,
  title = {Identifying the Prevalence, Correlates, and Policy Consequences of Anti-Vaccine Social Identity},
  author = {Motta, Matt and Callaghan, Timothy and Sylvester, Steven and Lunz-Trujillo, Kristin},
  date = {2021-05-30},
  journaltitle = {Politics, Groups, and Identities},
  volume = {0},
  number = {0},
  pages = {1--15},
  publisher = {{Routledge}},
  issn = {2156-5503},
  doi = {10.1080/21565503.2021.1932528},
  url = {https://doi.org/10.1080/21565503.2021.1932528},
  urldate = {2021-09-14},
  abstract = {Scholarly and journalistic profiles of anti-vaxxers – i.e., individuals who are active in efforts to oppose widespread vaccination – suggest that some Americans may identify with the “anti-vaccine” label in order to fulfill social goals (e.g., a sense of belonging in a broader community). This is potentially problematic, as anti-vaxx social identification (AVSID) could imply increased receptivity to vaccine misinformation, and resistance to evidence-based medicine. In a large and demographically representative survey (N = 1001), we propose a novel measure of AVSID, and take stock of its prevalence and correlates. We find that about 22\% of Americans always (8\%) or sometimes (14\%) self-identify as “anti-vaxxers” (activists who support vaccine refusal), and that those who do tend to embrace the label as a form of social identity. We also find that people who score highly on our AVSID measure tend to be less trusting of scientific experts and more individualistic. Finally, predictive validation analyses suggest that – among self-identified anti-vaxxers – AVSID is associated with increased opposition to childhood vaccine requirements. We conclude by outlining how our AVSID measure can be implemented to inform future research on opposition to evidence-based medicine and related public policies.},
  keywords = {Health opinion,health policy,misinformation,social identification,vaccine opinion},
  annotation = {\_eprint: https://doi.org/10.1080/21565503.2021.1932528},
  file = {C\:\\Users\\ckgon\\Zotero\\storage\\WRPURPMM\\Motta et al. - 2021 - Identifying the prevalence, correlates, and policy.pdf;C\:\\Users\\ckgon\\Zotero\\storage\\99289LF7\\21565503.2021.html}
