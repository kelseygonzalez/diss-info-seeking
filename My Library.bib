}

@article{ellisRoleHealthLiteracy2012,
  title = {The {{Role}} of {{Health Literacy}} and {{Social Networks}} in {{Arthritis Patients}}' {{Health Information-Seeking Behavior}}: {{A Qualitative Study}}},
