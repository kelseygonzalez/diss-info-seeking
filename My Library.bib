@article{hurlbertCoreNetworksTie2000,
  title = {Core {{Networks}} and {{Tie Activation}}: {{What Kinds}} of {{Routine Networks Allocate Resources}} in {{Nonroutine Situations}}?},
  shorttitle = {Core {{Networks}} and {{Tie Activation}}},
  author = {Hurlbert, Jeanne S. and Haines, Valerie A. and Beggs, John J.},
  date = {2000},
