  langid = {english},
  file = {C\:\\Users\\ckgon\\Zotero\\storage\\CIXLHJVK\\Lewis et al. - 2012 - Social selection and peer influence in an online s.pdf;C\:\\Users\\ckgon\\Zotero\\storage\\N9RJH3BP\\68.html}
